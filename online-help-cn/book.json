{
    "title": "XSCALEAR 联机帮助",
    "description": "关于 XSCALEAR 的联机帮助文档，旨在帮助用户了解一体机",
    "language": "zh-hans",
    "plugins": [
        "search-title",
        "favicon",
        "versions",
        "tbfed-pagefooter", 
        "copy-code-button", 
        "anchors", 
        "-sharing",
        "-lunr",
        "-search",
        "custom-js-css",
        "search-plus",
        "back-to-top-button",
        "expandable-chapters-small",
        "emphasize",
        "splitter",
        "popup",
        "edit-link",
        "hide-element"
    ],
    "styles": {
        "website": "styles/website.css"
    },

    "pluginsConfig":{
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy XSKY",
            "modify_label": "本文档更新于：",
            "modify_format": "YYYY-MM-DD HH:mm"
        },
        "custom-js-css": {
            "js": [
                "./assets/js/vendor/jquery/jquery-3.3.1.min.js",
                "./assets/js/main.js"
            ]
        },
        "versions": {
            "type": "languages",
            "options": [
                {
                    "value": "/zh-cn",
                    "text": "中文",
                    "selected": true
                },
                {
                    "value": "/en",
                    "text": "English"
                }
            ]
        },
        "favicon": {
            "shortcut": "assets/images/favicon.ico",
            "bookmark": "assets/images/favicon.ico",
            "appleTouch": "assets/images/logo.png"
        },
        "edit-link": {
            "base": "https://github.com/",
            "label": "编辑本页面"
        }
    }
}